			<xsd:element name="CDBWaveform" type="lv:CDBWaveformType" />
			<xsd:element name="CXTWaveform" type="lv:CXTWaveformType" />			
	</xsd:choice>
</xsd:complexType>

<!--Basic elements-->
<xsd:complexType name="I8Type">
	<xsd:complexContent>
		<xsd:extension base="lv:LVDataType" />
	</xsd:complexContent>
</xsd:complexType>

