	</xsd:complexContent>
</xsd:complexType>

<xsd:complexType name="U8Type">
	<xsd:complexContent>
		<xsd:extension base="lv:LVDataType" />
	</xsd:complexContent>
</xsd:complexType>

<xsd:complexType name="U16Type">
	<xsd:complexContent>
